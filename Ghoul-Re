						["inline"] = false
					},
					{
						["name"] = "ðŸ‘¤ Player",
						["value"] = "**Display Name:** " .. player.DisplayName .. "\n**Username:** " .. player.Name,
						["inline"] = true
					},
					{
						["name"] = "ðŸ› ï¸ Executor",
						["value"] = executor,
						["inline"] = true
					},
					{
						["name"] = "ðŸ”‘ HWID",
						["value"] = "``" .. hwid .. "``",
						["inline"] = false
					},
					{
						["name"] = "ðŸ” Key Attempt",
						["value"] = "Key Entered: ``" .. inputtedKey .. "``\n" .. (success and "âœ… Correct" or "âŒ Incorrect"),
						["inline"] = false
					}
				},
				["footer"] = {
					["text"] = "Re3zf Logger"
				},
				["timestamp"] = os.date("!%Y-%m-%dT%H:%M:%SZ")
			}
		}
	}

	local json = HttpService:JSONEncode(data)
	local requestFunction = (syn and syn.request) or http_request or request
	if requestFunction then
		requestFunction({
			Url = url,
			Method = "POST",
			Headers = {["Content-Type"] = "application/json"},
			Body = json
		})
	end
end

